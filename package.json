  "dependencies": {},
  "devDependencies": {
    "fixpack": "4.0.0",
    "markdown-pdf": "11.0.0",
    "textlint": "15.2.3",
    "textlint-filter-rule-allowlist": "4.0.0",
    "textlint-rule-preset-ja-spacing": "2.4.3",
    "textlint-rule-preset-ja-technical-writing": "12.0.2",
    "textlint-rule-spellcheck-tech-word": "5.0.0"
  },
  "license": "MIT",
  "main": "index.js",
  "repository": "git@github.com:kanade0404/resume.git",
  "scripts": {
    "fix": "fixpack",
    "generate": "for f in *.md; do out=\"${f%.md}.pdf\"; echo \"Generating $out\"; markdown-pdf \"$f\" -s ./style.css -o \"$out\"; done",
    "lint": "textlint *.md",
    "lint:fix": "textlint --fix *.md"
  },
  "type": "module"
}
